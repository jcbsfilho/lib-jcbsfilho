{
  "name": "lib-jcbsfilho",
  "version": "0.0.0",
  "description": "Lib Test",
  "scripts": {
    "prepare": "husky",
    "compile": "npm run compile --workspaces",
    "lint": "npm run lint --workspaces --if-present",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "format": "npm run prettier --workspaces --if-present",
    "format:check": "npm run prettier:check --workspaces --if-present",
    "build": "npm run compile"
  },
  "keywords": [
    "packages"
  ],
  "author": "jcbsfilho",
  "license": "MIT",
  "contributors": [
    {
      "name": "JosÃ© Filho",
      "url": "https://github.com/jcsbfilho"
    }
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.36.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "conventional-changelog-conventionalcommits": "^4.6.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "semantic-release": "^22.0.12",
    "tsup": "^8.5.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.44.1"
  },
  "workspaces": [
    "packages/*"
  ],
  "files": [
    "README.md",
    "package.json",
    "packages/**/dist/**/*"
  ],
  "exports": {
    ".": {
      "require": "./packages/client/dist/index.js",
      "import": "./packages/client/dist/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "client": [
        "./packages/client/dist/index.d.ts"
      ]
    }
  },
  "dependencies": {
  }
}
